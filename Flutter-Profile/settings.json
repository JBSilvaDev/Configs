{

  "files.exclude": {
    "._.DS_Store": true,
    "._.idea": true,
    "._**": true,
    ".dart_tool": true,
    ".idea": true,
    ".vscode": true,
    "**._.dart_tool": true
  },

  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "git.confirmSync": false,

  "powermode.enabled": true,
